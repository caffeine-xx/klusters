   <Action name="decrease_all_channels" /> 
   <Separator/>
   <Action name="show_next_cluster" />
   <Action name="show_previous_cluster" /> 
   <Separator/>   
   <Action name="show_labels"/>    
  </Menu>
  
  <Menu name="display_menu" >
