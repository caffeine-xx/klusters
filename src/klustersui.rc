   <Separator/>
   <Action name="rename_display" />
   <Separator/>
   <Action name="close_display" />
  </Menu>
                  
  <Menu name="settings" >
   <Action append="show_toolbar_merge" name="show_actionBar" />
   <Action append="show_toolbar_merge" name="show_toolBar" />
   <Action append="show_toolbar_merge" name="show_parameterBar" />
	<Action append="show_toolbar_merge" name="show_clusterInfo" />	
   <Action append="save_merge" name="immediate_selection" />
   <Action append="save_merge" name="delay_selection" />
  </Menu>
 </MenuBar>
 
 <ToolBar name="actionBar" >
